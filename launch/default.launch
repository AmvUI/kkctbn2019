<launch>

    <node pkg="kkctbn2019" name="command" type="command" output="screen"/>

    <node pkg="kkctbn2019" name="motor_controller" type="motor_controller" output="screen"/>

    <node pkg="kkctbn2019" name="mode" type="mode" output="screen"/>

    <node pkg="kkctbn2019" name="receiver" type="receiver.py" output="screen"/>

    <node pkg="kkctbn2019" name="image_proccessing" type="image_proccessing.py" output="screen"/>

    <node pkg="pid" name="setpoint" type="setpoint_node" output="screen"/>
    
    <node name="controller" pkg="pid" type="controller" >
      <param name="Kp" value="0.4" />
      <param name="Ki" value="0.5" />
      <param name="Kd" value="0.5" />
      <param name="upper_limit" value="300.0" />
      <param name="lower_limit" value="-300.0" />
      <param name="windup_limit" value="100.0" />
      <param name="max_loop_frequency" value="100.0" />
      <param name="min_loop_frequency" value="100.0" />
    </node>
</launch>
